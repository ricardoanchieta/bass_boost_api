# Configurações da Bass Boost API
# Copie este arquivo para .env e ajuste conforme necessário

# Informações da aplicação
APP_NAME=Bass Boost API
APP_DESCRIPTION=API para aumentar o grave de músicas MP3
APP_VERSION=1.0.0

# Configurações do servidor
HOST=0.0.0.0
PORT=8000
DEBUG=true
RELOAD=true

# Configurações de upload
MAX_FILE_SIZE=52428800  # 50MB em bytes
ALLOWED_EXTENSIONS=["mp3"]

# Configurações de processamento de áudio
DEFAULT_BASS_BOOST=5
MIN_BASS_BOOST=-50
MAX_BASS_BOOST=50

# CORS
CORS_ORIGINS=["*"]
CORS_METHODS=["GET", "POST"]
CORS_HEADERS=["*"]
